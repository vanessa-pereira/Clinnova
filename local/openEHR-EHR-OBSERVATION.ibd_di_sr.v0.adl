definition
	OBSERVATION[at0000] matches {    -- Perceived Stress Scale (PSS)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					INTERVAL_EVENT[at0002] occurrences matches {0..*} matches {    -- In the last month
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Have been upset because of something that happened unexpectedly
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Have felt that was unable to control the important things in life
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {    -- Have felt nervous and stressed
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Have felt confident about ability to handle personal problems
										value matches {
											4|[local::at0026],
											3|[local::at0027],
											2|[local::at0028],
											1|[local::at0029],
											0|[local::at0030]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Have felt that things were going in the desired way
										value matches {
											4|[local::at0026],
											3|[local::at0027],
											2|[local::at0028],
											1|[local::at0029],
											0|[local::at0030]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Have found that could not cope with all the things that had to do
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0015] occurrences matches {0..1} matches {    -- Have been able to control irritations in life
										value matches {
											4|[local::at0026],
											3|[local::at0027],
											2|[local::at0028],
											1|[local::at0029],
											0|[local::at0030]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Have felt that were on top of things
										value matches {
											4|[local::at0026],
											3|[local::at0027],
											2|[local::at0028],
											1|[local::at0029],
											0|[local::at0030]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Have been angered because of things that happened outside of patient control
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Have felt difficulties were piling up high that could not overcome them
										value matches {
											0|[local::at0005],
											1|[local::at0006],
											2|[local::at0007],
											3|[local::at0008],
											4|[local::at0009]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..40|}
											}
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Total score interpretation
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0023,    -- Low stress
													at0024,    -- Moderate stress
													at0025]    -- High perceived stress
												}
											}
										}
									}
								}
							}
						}
						width matches {
							DV_DURATION matches {
								value matches {P1M}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0019] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0020] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

