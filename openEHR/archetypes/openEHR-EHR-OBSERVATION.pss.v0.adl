					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Ont été bouleversés à cause de quelque chose qui s'est produit de façon inattendue">
					description = <"Question originale: \"Au cours du dernier mois, à quelle fréquence avez-vous été bouleversé à cause d'un événement inattendu?\"">
				>
