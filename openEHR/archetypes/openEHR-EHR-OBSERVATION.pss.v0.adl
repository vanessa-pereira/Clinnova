		["fr"] = <
			items = <
				["at0000"] = <
					text = <"Échelle de stress perçu (PSS)">
					description = <"L'échelle de stress perçu (PSS-10) est un questionnaire en 10 items développé à l'origine par Cohen et al. (1983) largement utilisé pour évaluer les niveaux de stress chez les jeunes et les adultes âgés de 12 ans et plus. Il évalue dans quelle mesure un individu a perçu la vie comme imprévisible, incontrôlable et surchargée au cours du mois précédent.">
				>
				["at0001"] = <
					text = <"History">
					description = <"@ internal @">
