definition
	OBSERVATION[at0000] matches {    -- EULAR/ACR Classification Criteria for SLE 2019
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0005] occurrences matches {0..1} matches {    -- Clinical domains and criteria
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0011] occurrences matches {0..1} matches {    -- Constitutional
												value matches {
													2|[local::at0012]
												}
											}
											ELEMENT[at0013] occurrences matches {0..3} matches {    -- Hematologic
												value matches {
													3|[local::at0014],
													4|[local::at0015],
													4|[local::at0016]
												}
											}
											ELEMENT[at0017] occurrences matches {0..3} matches {    -- Neuropsychiatric
												value matches {
													2|[local::at0018],
													3|[local::at0019],
													5|[local::at0020]
												}
											}
											ELEMENT[at0021] occurrences matches {0..4} matches {    -- Mucocutaneous
												value matches {
													2|[local::at0022],
													2|[local::at0023],
													4|[local::at0024],
													6|[local::at0025]
												}
											}
											ELEMENT[at0026] occurrences matches {0..2} matches {    -- Serosal
												value matches {
													5|[local::at0027],
													6|[local::at0028]
												}
											}
											ELEMENT[at0029] occurrences matches {0..1} matches {    -- Musculoskeletal
												value matches {
													6|[local::at0030]
												}
											}
											ELEMENT[at0031] occurrences matches {0..3} matches {    -- Renal
												value matches {
													4|[local::at0032],
													8|[local::at0033],
													10|[local::at0034]
												}
											}
											ELEMENT[at0044] occurrences matches {0..1} matches {    -- Score
												value matches {
													DV_COUNT matches {*}
												}
											}
										}
									}
									CLUSTER[at0006] occurrences matches {0..1} matches {    -- Immunological domains and criteria
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0035] occurrences matches {0..1} matches {    -- Antiphospholipid antibodies
												value matches {
													2|[local::at0036]
												}
											}
											ELEMENT[at0037] occurrences matches {0..2} matches {    -- Complement proteins
												value matches {
													3|[local::at0038],
													4|[local::at0039]
												}
											}
											ELEMENT[at0040] occurrences matches {0..1} matches {    -- SLE-specific antibodies
												value matches {
													6|[local::at0041]
												}
											}
											ELEMENT[at0045] occurrences matches {0..1} matches {    -- Score
												value matches {
													DV_COUNT matches {*}
												}
											}
										}
									}
									ELEMENT[at0007] matches {    -- Total score
										value matches {
											DV_COUNT matches {*}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Classification
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0009,    -- SLE Classification Criteria not met
													at0010]    -- SLE Classification Criteria fulfilled
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0043] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

