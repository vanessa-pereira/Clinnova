										}
									}
									CLUSTER[at0010] matches {    -- Neuropsychiatric
										items cardinality matches {5..5; unordered} matches {
											ELEMENT[at0011] matches {    -- Cognitive impairment or major psychosis
												value matches {
													0|[local::at0006],
													1|[local::at0007]
												}
											}
