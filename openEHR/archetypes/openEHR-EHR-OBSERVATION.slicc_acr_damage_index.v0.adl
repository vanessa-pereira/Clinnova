									CLUSTER[at0018] matches {    -- Renal
										items cardinality matches {3..3; unordered} matches {
											ELEMENT[at0019] matches {    -- Estimated or measured GFR < 50%
												value matches {
													0|[local::at0006],
													1|[local::at0007]
