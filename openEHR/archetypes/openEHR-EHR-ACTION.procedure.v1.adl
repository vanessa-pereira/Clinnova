					description = <"*The planned procedure has been cancelled prior to commencement.(en)">
				>
				["at0040"] = <
					text = <"*Procedure suspended(en)">
					description = <"*The procedure has been suspended.(en)">
				>
				["at0041"] = <
					text = <"*Procedure aborted(en)">
