					description = <"*The procedure has been performed and all associated clinical activities completed.(en)">
				>
				["at0047"] = <
					text = <"*Procedure performed(en)">
					description = <"*The procedure, or subprocedure in a multicomponent procedure, has been performed.(en)">
				>
				["at0048"] = <
					text = <"*Outcome(en)">
					description = <"*Outcome of procedure performed.(en)">
					comment = <"*Coding with a terminology is preferred, where possible.(en)">
				>
				["at0049"] = <
					text = <"*Description(en)">
					description = <"*Narrative description about the procedure, as appropriate for the pathway step.(en)">
