definition
	CLUSTER[at0000] matches {    -- Specimen
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0029] occurrences matches {0..1} matches {    -- Specimen type
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0098] occurrences matches {0..1} matches {    -- Specimen label
				value matches {
					DV_TEXT matches {*}
